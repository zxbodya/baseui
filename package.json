    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.0.0",
    "jest-enzyme": "^7.0.1",
    "jest-matcher-utils": "^24.0.0",
    "jest-puppeteer": "^4.0.0",
    "jscodeshift": "^0.6.4",
    "markdownlint-cli": "^0.18.0",
    "memory-fs": "^0.4.1",
    "mkdirp": "^0.5.1",
    "next": "^9.0.5",
    "next-images": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "pretty-proptypes": "^1.0.0",
    "pretty-quick": "^1.4.1",
    "prism-react-renderer": "^0.1.7",
    "puppeteer": "^1.19.0",
    "raw-loader": "^3.1.0",
    "react": "^16.8.6",
    "react-codesandboxer": "^3.1.1",
    "react-dom": "^16.8.6",
    "react-live": "^2.1.2",
    "react-markdown": "^4.0.3",
    "react-vis": "^1.11.6",
    "recursive-readdir": "^2.2.2",
    "remove-flow-types-loader": "^1.1.0",
    "screener-storybook": "^0.18.6",
    "semver": "^6.2.0",
    "shx": "^0.3.2",
    "static-server": "^2.2.1",
    "storybook-addon-rtl": "^0.2.2",
    "styletron-engine-atomic": "^1.4.0",
    "styletron-react": "^5.2.0",
    "styletron-standard": "^3.0.0",
    "tar": "^4.4.8",
    "terser": "^4.1.4",
    "typescript": "^3.5.3",
    "webpack": "^4.17.1",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.1",
    "card-validator": "^6.1.0",
    "date-fns": "2.1.0",
    "just-extend": "^4.0.2",
    "memoize-one": "^5.0.0",
    "polished": "^3.2.0",
    "popper.js": "^1.14.3",
    "react-dropzone": "^9.0.0",
    "react-focus-lock": "^1.9.1",
    "react-input-mask": "^2.0.4",
    "react-is": "^16.8.6",
    "react-movable": "^2.2.0",
    "react-multi-ref": "^1.0.0",
    "react-range": "^1.3.0",
    "react-virtualized": "^9.21.1",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "smoothscroll-polyfill": "^0.4.3",
    "timezone-support": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">= 16.8.0 < 17",
    "react-dom": ">= 16.8.0 < 17",
    "styletron-react": "^5.2.1"
  },
  "resolutions": {
    "@types/react": "16.8.23"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
